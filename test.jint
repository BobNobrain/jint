@Interface
{
	imports: [ "std:basicInterface.jinc", "myClasses.jinc" ],
	name: "MyInterface",
	type: -WINDOW, version: @Version { standart: 1, description: "jint 0.01 alpha" },
	elements:
	[
		@Page("Page Title", "page")
		{
			padding: 10,
			elements:
			[
				@Label("Click the button!", null, auto) { sizeSource: -FLOW },
